			<xsd:sequence minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="DatovySoubor" type="DatSouborKodVerType"/>
				<xsd:element name="Vyskyt" type="VyskytType"/>
				<xsd:element name="Vydani" type="VydaniType"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="EwiWSResult">
		<xsd:annotation>
			<xsd:documentation>Výsledek zpracování </xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Dotaz"/>
				<xsd:choice minOccurs="0">
