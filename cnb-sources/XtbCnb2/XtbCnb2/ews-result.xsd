		<xsd:annotation>
			<xsd:documentation>Dotaz typu: Obsah datového souboru</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="Metodika" type="MetodikaKodType"/>
				<xsd:element name="DatovySoubor" type="DatSouborKodType"/>
				<xsd:element name="DatovaOblast" type="DatOblastType"/>
